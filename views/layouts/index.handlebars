<div class="container-fluid mt-3">
    <h3>{{title}} </h3>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">season</th>
                <th scope="col">Total Repaid</th>
                <th scope="col">Total Credit </th>
                <th scope="col">Client </th>
                <th scope="col">Season Start </th>
                <th scope="col">Season End </th>
            </tr>
        </thead>
        <tbody>
            {{#each paymentsInfo}}
                <tr>
                    {{#with this}}
                        <td>{{summaryId}}</td>
                        <td>{{seasonName}}</td>
                        <td>{{totalRepaid}}</td>
                        <td>{{totalCredit}}</td>
                        <td>{{customerName}}</td>
                        <td>{{startDate}}</td>
                        <td>{{endDate}}</td>
                    {{/with}}
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>